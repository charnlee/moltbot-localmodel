{
  "name": "@charnlee/vllm-local",
  "version": "0.2.0",
  "type": "module",
  "description": "Moltbot plugin for connecting to locally deployed vLLM models",
  "author": "chengli <charnlee@163.com>",
  "license": "MIT",
  "keywords": [
    "moltbot",
    "vllm",
    "local-model",
    "openai-compatible",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charnlee/moltbot-localmodel.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "clawdbot.plugin.json",
    "moltbot.d.ts",
    "README.md",
    "QUICKSTART.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run rebuild"
  },
  "moltbot": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "peerDependencies": {
    "moltbot": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "moltbot": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "moltbot": "^0.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
